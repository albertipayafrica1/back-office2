[
  {
    "logConfiguration": {
      "logDriver": "awslogs",
      "options": {
        "awslogs-group": "/ecs/apis-<DEPLOYMENT_ENVIRONMENT>-backofficev4",
        "awslogs-region": "eu-west-1",
        "awslogs-stream-prefix": "ecs"
      }
    },
    "portMappings": [
      {
        "hostPort": 0,
        "protocol": "tcp",
        "containerPort": 3000
      }
    ],
    "cpu": 0,
    "memoryReservation": 128,
    "environment": [
      {
        "name": "BUILD_NUMBER",
        "value": "<IMAGE_VERSION>"
      },
      {
        "name": "port",
        "value": "8080"
      }
    ],
    "mountPoints": [],
    "volumesFrom": [],
    "image": "202492758425.dkr.ecr.eu-west-1.amazonaws.com/elipa/backoffice-v4:<IMAGE_VERSION>",
    "name": "app",
    "secrets": []
  }
]
